[*]
charset=utf-8
end_of_line=lf
insert_final_newline=false
indent_style=space
indent_size=4

[{.babelrc,.stylelintrc,.eslintrc,jest.config,*.bowerrc,*.jsb3,*.jsb2,*.json}]
indent_style=tab
tab_width=4

[*.java]
indent_style=tab
tab_width=4

[{*.ddl,*.sql}]
indent_style=space
indent_size=2

[{*.gson,*.gradle,*.groovy,*.gant,*.gdsl,*.gy}]
indent_style=tab
tab_width=4

[{*.yml,*.yaml}]
indent_style=space
indent_size=2

